{
    "manifest_version": 2,
    "name": "Jean-Laurent Launcher",
    "description": "Quick launch Adam Jean-Laurent's Media",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default _icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "<all_urls>"]
}